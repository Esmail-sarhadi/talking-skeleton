<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context=".middlePart"
        android:layoutDirection="ltr"
        android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/talkingskeleton"
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:src="@drawable/skeleton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="?attr/actionBarSize"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/midbtn1"
            android:layout_width="match_parent"
            android:layout_height="170dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/rounded_corner_view"
            android:text="قسمت میانی اسکلت"
            android:fontFamily="@font/iransans"
            android:textColor="@color/black"
            android:gravity="center"
            android:textSize="31sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/talkingskeleton"
            android:layout_marginTop="40dp"/>

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/jenaq.sine"
            android:layout_width="90dp"
            android:layout_height="0dp"
            android:layout_marginRight="20dp"
            app:shapeAppearanceOverlay="@style/RoundedSquare"
            android:scaleType="fitXY"
            android:src="@drawable/sternum"
            app:layout_constraintBottom_toBottomOf="@+id/jenaq.sine.en"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/jenaq.sine.fa" />

        <Button
            android:id="@+id/jenaq.sine.fa"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="40dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="جناغ سینه"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/jenaq.sine"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/midbtn1" />

        <Button
            android:id="@+id/jenaq.sine.en"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="0dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="sternum"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/jenaq.sine"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/jenaq.sine.fa" />


        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/dandeha"
            android:layout_width="90dp"
            android:layout_height="0dp"
            android:layout_marginRight="20dp"
            android:scaleType="fitXY"
            android:src="@drawable/ribs"
            app:shapeAppearanceOverlay="@style/RoundedSquare"
            app:layout_constraintBottom_toBottomOf="@+id/dandeha.en"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/dandeha.fa" />


        <Button
            android:id="@+id/dandeha.fa"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="دنده ها"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/dandeha"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/jenaq.sine.en" />


        <Button
            android:id="@+id/dandeha.en"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="0dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="ribs"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/dandeha"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dandeha.fa" />

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/sotun.mohreha"
            android:layout_width="90dp"
            android:layout_height="0dp"
            android:layout_marginRight="20dp"
            app:shapeAppearanceOverlay="@style/RoundedSquare"
            android:scaleType="fitXY"
            android:src="@drawable/vertebralcolumn"
            app:layout_constraintBottom_toBottomOf="@+id/sotun.mohreha.en"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/sotun.mohreha.fa" />


        <Button
            android:id="@+id/sotun.mohreha.fa"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="ستون مهره ها"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/sotun.mohreha"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dandeha.en" />


        <Button
            android:id="@+id/sotun.mohreha.en"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="0dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="vertebral column"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/sotun.mohreha"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/sotun.mohreha.fa" />


        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/bazou"
            android:layout_width="90dp"
            android:layout_height="0dp"
            android:layout_marginRight="20dp"
            android:scaleType="fitXY"
            app:shapeAppearanceOverlay="@style/RoundedSquare"
            android:src="@drawable/humerus"
            app:layout_constraintBottom_toBottomOf="@+id/bazou.en"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/bazou.fa" />


        <Button
            android:id="@+id/bazou.fa"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="بازو"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/bazou"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/sotun.mohreha.en" />


        <Button
            android:id="@+id/bazou.en"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="0dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="humerus"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/bazou"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bazou.fa" />

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/saead"
            android:layout_width="90dp"
            android:layout_height="0dp"
            android:layout_marginRight="20dp"
            android:scaleType="fitXY"
            app:shapeAppearanceOverlay="@style/RoundedSquare"
            android:src="@drawable/forearm"
            app:layout_constraintBottom_toBottomOf="@+id/saed.en"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/saed.fa" />


        <Button
            android:id="@+id/saed.fa"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"

            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="ساعد"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/saead"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bazou.en" />

        <Button
            android:id="@+id/saed.en"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="0dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="forearm"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/saead"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/saed.fa" />


        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/dast"
            android:layout_width="90dp"
            android:layout_height="0dp"
            android:layout_marginRight="20dp"
            android:scaleType="fitXY"
            app:shapeAppearanceOverlay="@style/RoundedSquare"
            android:src="@drawable/wristandfingers"
            app:layout_constraintBottom_toBottomOf="@+id/dast.en"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/dast.fa" />

        <Button
            android:id="@+id/dast.fa"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="مچ و انگشتان دست"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/dast"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/saed.en" />


        <Button
            android:id="@+id/dast.en"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="0dp"
            android:backgroundTint="#9FC53A"
            android:fontFamily="@font/iransanslight"
            android:text="wrist and fingers"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/dast"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dast.fa" />





    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>
